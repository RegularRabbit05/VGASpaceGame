#define COUNTERMAX 10000

__attribute__((unused)) void loop() {
  switch (demo%11) {
    case 0:
      vga.putpixel((cnt/(COUNTERMAX/VGAX_BWIDTH))*8+vga.rand()%8, vga.rand()%VGAX_HEIGHT, vga.rand()%2);
      break;
    case 1:
      vga.setExtendedColorsMask(cnt/1000);
      vga.putpixel(VGAX_ECSWIDTH+vga.rand()%(VGAX_WIDTH-VGAX_ECSWIDTH*2), vga.rand()%VGAX_HEIGHT, vga.rand()%2);
      break;
