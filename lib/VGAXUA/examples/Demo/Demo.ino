      vga.printPROGMEM((byte*)fnt_nanofont_data, FNT_NANOFONT_SYMBOLS_COUNT, FNT_NANOFONT_HEIGHT, 3, 1, str0, 0, 6*(cnt/(COUNTERMAX/10)), 1);
      cnt+=50;
      break;
    case 3:
      VGAXUAUtils::draw_circle_safe(vga.rand()%VGAX_WIDTH, vga.rand()%VGAX_HEIGHT, vga.rand()%20, vga.rand()%2, -1);
      cnt+=5;
      break;
    case 4:
      VGAXUAUtils::draw_line_safe(vga.rand()%(VGAX_WIDTH+10)-5, vga.rand()%(VGAX_HEIGHT+10)-5,
                                  vga.rand()%(VGAX_WIDTH+10)-5, vga.rand()%(VGAX_HEIGHT+10)-5, vga.rand()%2);
      cnt+=10;
      break;
    case 5:
      VGAXUAUtils::draw_rect_safe(vga.rand()%(VGAX_WIDTH+10)-5, vga.rand()%(VGAX_HEIGHT+10)-5, vga.rand()%60, vga.rand()%60, vga.rand()%2, -1);
      cnt+=6;
      break;
    case 6:
      vga.bitblit((byte*)img_data, IMG_WIDTH, IMG_HEIGHT, vga.rand()%(VGAX_WIDTH+10)-5, vga.rand()%(VGAX_HEIGHT+10)-5, vga.rand()%2);
      cnt+=3;
      break;
    case 7:
      vga.blit((byte*)img_data, IMG_WIDTH, IMG_HEIGHT, vga.rand()%(VGAX_WIDTH+10)-5, vga.rand()%(VGAX_HEIGHT+10)-5);
      cnt+=3;
      break;
    case 8:
      vga.blitwmask((byte*)img_data, (byte*)img_data, IMG_WIDTH, IMG_HEIGHT, vga.rand()%(VGAX_WIDTH+10)-5, vga.rand()%(VGAX_HEIGHT+10)-5);
      cnt+=5;
      break;
    case 9:
      #if defined(__AVR_ATmega2560__)
      vga.copy((byte*)img_pirate_data);
      #else
      vga.copy((byte*)img_elaine_data);
      #endif
      cnt+=30;
      break;
    case 10:
      if (cnt==0) {
        #if defined(__AVR_ATmega2560__)
        vga.copy((byte*)img_pirate_data);
        #else
        vga.copy((byte*)img_elaine_data);
        #endif
        VGAXUAUtils::draw_rect(0, 0, VGAX_ECSWIDTH, VGAX_HEIGHT, 0, 0);
        VGAXUAUtils::draw_rect(VGAX_WIDTH-VGAX_ECSWIDTH-1, 0, VGAX_ECSWIDTH, VGAX_HEIGHT, 0, 0);
      }
      vga.setExtendedColorsMask(cnt/1000);
      vga.delay(1);
      break;
  }
  cnt++;
  if (cnt>COUNTERMAX) {
    vga.setExtendedColorsMask(0);
    vga.clear(0);
    cnt=0;
    demo++;
  }
}
